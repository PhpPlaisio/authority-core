{
  "name": "plaisio/authority-core",
  "description": "PhpPlaisio: Core Authority",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Authority"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "plaisio/authority": "^1.3.0",
    "plaisio/company-resolver": "^2.1.0",
    "plaisio/db-page": "^0.9.2",
    "plaisio/kernel": "^3.2.1",
    "plaisio/session": "^5.0.0",
    "setbased/php-stratum-mysql": "^6.6.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.3",
    "plaisio/console-kernel": "^1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Authority\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
