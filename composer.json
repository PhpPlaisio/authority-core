{
  "name": "plaisio/authority-core",
  "description": "PhpPlaisio: Core Authority",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Authority"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/authority": "^1.0",
    "plaisio/kernel": "^1.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Authority\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Authority\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
