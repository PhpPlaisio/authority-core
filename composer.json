{
  "name": "plaisio/authority-core",
  "description": "PhpPlaisio: Core Authority",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Authority"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "plaisio/authority": "^1.2.0",
    "plaisio/company-resolver": "^2.0.0",
    "plaisio/db-page": "^0.9.1",
    "plaisio/kernel": "^3.1.1",
    "plaisio/session": "^4.0.0",
    "setbased/php-stratum-mysql": "^6.5.0"
  },
  "require-dev": {
    "phing/phing": "^2.17.2",
    "phpunit/phpunit": "^9.5.20",
    "plaisio/console": "^2.7.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Authority\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Authority\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
