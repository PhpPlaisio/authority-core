{
  "name": "setbased/abc-authority-core",
  "description": "ABC-Framework: Core Authority",
  "keywords": [
    "ABC-Framework",
    "Core",
    "Authority"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "setbased/abc": "*",
    "setbased/abc-authority": "*"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^6.0.0",
    "setbased/php-stratum": "*"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\Authority\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\Authority\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
