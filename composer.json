{
  "name": "plaisio/authority-core",
  "description": "PhpPlaisio: Core Authority",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Authority"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "plaisio/kernel": "^1.0.0",
    "plaisio/authority": "^1.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Authority\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Authority\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
