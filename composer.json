{
  "name": "plaisio/authority-core",
  "description": "PhpPlaisio: Core Authority",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Authority"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio/authority": "^1.4.0",
    "plaisio/company-resolver": "^2.2.0",
    "plaisio/db-page": "^0.9.3",
    "plaisio/kernel": "^3.3.0",
    "plaisio/session": "^5.1.0",
    "setbased/php-stratum-mysql": "^7.1.1"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "plaisio/console-kernel": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Authority\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
